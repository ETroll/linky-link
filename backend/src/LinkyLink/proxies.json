{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "API": {
      "matchCondition": {
        "route": "/api/{*url}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{url}"
    },
    "App": {
      "matchCondition": {
        "route": "/{*url}",
        "methods": ["GET", "HEAD", "OPTIONS"]
      },
      "backendUri": "https://theurlistdotcom.z19.web.core.windows.net/{url}"
    }
  }
}
